#include <iostream>
#include <algorithm>
using namespace std;

void solve(){
    int n,a,b,c;
    cin >> n >> a >> b >> c;
    if(b>=n && a+c>=n){
        cout << "YES" << endl;
    }
    else{
        cout << "NO" << endl;
    }
}

int main(){
	int t;
    cin >> t;
    while(t--){
        solve();
    }
    return 0;
}
